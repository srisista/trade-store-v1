<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://owasp.org/www-project-dependency-check/dependency-check-suppression.1.3.xsd">
    <!-- Spring Web vulnerabilities -->
    <suppress>
        <notes>Spring Web vulnerability that will be fixed in future releases</notes>
        <cve>CVE-2024-38809</cve>
        <cvssBelow>9.0</cvssBelow>
        <packageUrl regex="true">pkg:maven/org\.springframework/spring-web@6\.1\.12</packageUrl>
    </suppress>

    <!-- Spring WebMVC vulnerabilities -->
    <suppress>
        <notes>Spring WebMVC vulnerability that will be fixed in future releases</notes>
        <cve>CVE-2024-38816</cve>
        <cvssBelow>9.0</cvssBelow>
        <packageUrl regex="true">pkg:maven/org\.springframework/spring-webmvc@6\.1\.12</packageUrl>
    </suppress>

    <!-- Spring Core vulnerability -->
    <suppress>
        <notes>Spring Core vulnerability that will be fixed in future releases</notes>
        <cve>CVE-2024-38820</cve>
        <cvssBelow>9.0</cvssBelow>
        <packageUrl regex="true">pkg:maven/org\.springframework/spring-core@6\.1\.12</packageUrl>
    </suppress>

    <!-- Spring Boot Actuator vulnerability -->
